<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lmn.common.dao.UserDao">
    <sql id="result">
        role as role,
        name as name

    </sql>
   <select id="getByEntity" resultType="com.lmn.common.entity.User" parameterType="com.lmn.common.entity.User">
       SELECT * from t_lmn_sys_user where name=#{name}
   </select>
    <insert id="insert" parameterType="com.lmn.common.entity.User">
        INSERT INTO t_lmn_sys_user(
        id,
        name,
        password,
        login_name,
        create_by,
        create_date,
        update_by,
        update_date
        ) VALUES (
        #{id},
        #{name},
        #{password},
        #{loginName},
        #{createBy},
        #{createDate},
        #{updateBy},
        #{updateDate}
        )
    </insert>

</mapper>